{
  extends: [
    'plugin:vue/strongly-recommended', //n√≠vel da regra
  ],
  "parserOptions": {
    "parser": 'babel-eslint',
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "semi": "error",
    "vue/html-closing-bracket-newline": ["error", {
      "singleline": "never",
      "multiline": "never"
    }]
  }
}
